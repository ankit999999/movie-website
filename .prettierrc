{
  "overrides": [
    {
      "files": "*.jsx",
      "options": {
        "trailingComma": "none",
        "semi": false,
        "printWidth": 80,
        "tabWidth": 2,
        "useTabs": true,
        "singleQuote": true,
        "bracketSpacing": false,
        "explicitTypes": "never"
      }
    }
  ]
}
